{
    "name": "aristides/site-example",
    "description": "Projeto simples para iniciar um webiste",
    "authors": [
        {
            "name": "Aristides Neto",
            "email": "aristidesbneto@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "phpunit/phpunit": "^9.5",
        "illuminate/database": "^8.35",
        "illuminate/routing": "^8.35",
        "illuminate/events": "^8.35",
        "illuminate/container": "^8.35",
        "illuminate/contracts": "^8.35",
        "illuminate/filesystem": "^8.35",
        "illuminate/view": "^8.35",
        "vlucas/phpdotenv": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Aristides\\Framework\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "test/"
        },
        "files": [
            "src/Helpers/functions.php",
            "src/Helpers/view.php"
        ]
    },
    "scripts": {
        "serve": "php -S localhost:8080 -t public/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
